{
    "compilerOptions": {
        "jsx": "react",
        "target": "ESNext",
        "resolveJsonModule": true,
        "importHelpers": true,
        "module": "CommonJS",
        "declaration": true,
        "emitDeclarationOnly": false,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "preserveConstEnums": true,
        "removeComments": false,
        "noEmitHelpers": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "downlevelIteration": false,
        "baseUrl": "./",
        "rootDir": "./src"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
